                                  type="xs:string">
                        <xs:annotation>
                            <xs:documentation>List of BCC email
                                addresses separated by comma
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="ConfirmationArchiveRequest" type="xs:integer"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Should contain archive record ID (for internal
                        use : yrco_id)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute
                name="Name" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="ccm_book_actualsType">
        <xs:annotation>
            <xs:documentation>POS
                posting
            </xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="bookingID"
                        type="xs:integer"/>
            <xs:element name="event_id" type="xs:integer"/>
            <xs:element
                    name="postingDate" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Date in format
                        DD.MM.YYYY
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="menu_id"
                        type="xs:integer" minOccurs="0"/>
            <xs:element name="menu_item_id" type="xs:integer"
                        minOccurs="0"/>
            <xs:element name="misc_item_id" type="xs:integer" minOccurs="0"/>
            <xs:element
                    name="function_space_id" type="xs:integer" minOccurs="0"/>
            <xs:element name="ResourceName"
                        type="xs:string"/>
            <xs:element name="DepartmentCode" type="xs:string"/>
            <xs:element name="count"
                        type="xs:integer"/>
            <xs:element name="total_amount" type="xs:string"/>
            <xs:element
                    name="OverrideReason" type="xs:string" minOccurs="0"/>
            <xs:element name="UserName"
                        type="xs:string"/>
            <xs:element name="Covers" type="xs:integer" minOccurs="0"/>
        </xs:all>
    </xs:complexType>
    <xs:complexType name="crm_postType">
        <xs:annotation>
            <xs:documentation>Used to submit a
                posting
            </xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="GuestNum"
                        type="xs:integer">
                <xs:annotation>
                    <xs:documentation>Reservation number</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PostingDate" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Date in format DD.MM.YYYY</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RoomNum" type="xs:string"/>
            <xs:element
                    name="Quantity" type="xs:integer"/>
            <xs:element name="Price" type="xs:string"/>
            <xs:element
                    name="Comment" type="xs:string" minOccurs="0"/>
            <xs:element name="CheckNum" type="xs:string"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Is mandatory if Check element is
                        submited
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CheckDate" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Used only together with
                        Check. It is mandatory if Check element is submited. Must contain date and time part
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Check" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>POS Guest Checks</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Description" type="xs:string" minOccurs="0"/>
            <xs:element name="Currency"
                        type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Optional currency code short
                        description. If not provided, postig made in system default currency
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ProfileID" type="xs:integer" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Allows to make a posting for specific guest in multi guest
                        reservation. Not supported for FA postings
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
